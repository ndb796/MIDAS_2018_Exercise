<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="0dp"
        android:right="1dp"
        android:left="-1dp"
        android:top="-1dp"
        >
        <shape android:shape="rectangle" >
            <corners android:radius="5dip"/>

            <stroke
                android:width="2dp"
                android:color="#585858" />

            <solid android:color="#FFF" />
        </shape>
    </item>

</layer-list>